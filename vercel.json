{
  "version": 2,
  "builds": [
    {
      "src": "static/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/static/index.html"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/css/(.+).css",
      "destination": "/static/css/$1.css"
    },
    {
      "source": "/js/(.+).js",
      "destination": "/static/js/$1.js"
    }
  ]
}